{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
      {
          "name": "Best Buy inc.",
          "num_employees": 200,
          "description": "Electronics and Appliance Retailer",
          "logo_url": "www.bb_logo_url.com"
      }
  ],
  "required": [
      "name",
      "num_employees",
      "description",
      "logo_url"
  ],
  "properties": {
      "name": {
          "$id": "#/properties/name",
          "type": "string",
          "title": "The name schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "Best Buy inc."
          ]
      },
      "num_employees": {
          "$id": "#/properties/num_employees",
          "type": "integer",
          "title": "The num_employees schema",
          "description": "An explanation about the purpose of this instance.",
          "default": 0,
          "examples": [
              200
          ]
      },
      "description": {
          "$id": "#/properties/description",
          "type": "string",
          "title": "The description schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "Electronics and Appliance Retailer"
          ]
      },
      "logo_url": {
          "$id": "#/properties/logo_url",
          "type": "string",
          "title": "The logo_url schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "www.bb_logo_url.com"
          ]
      }
  },
  "additionalProperties": true
}